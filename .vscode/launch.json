  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch API",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/api/main.go"
    },
    {
      "name": "Debug Local Docker",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "port": 40000,
      "host": "localhost",
      "apiVersion": 2
    },
    {
      "name": "Debug Remote Docker",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "port": 40000,
      "host": "https://proxy.stronger.software",
      "apiVersion": 2
    }
  ]
}
